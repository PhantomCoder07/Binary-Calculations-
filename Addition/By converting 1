#include <stdio.h>
#include <math.h>
int convertb (int n)
{
    int i=0,sum=0;
    while (n!=0)
    {
        int d=n%10;
        if (d==1)
        {
            sum+=pow(2,i);
        }
        n/=10;
        i++;
    }
    return sum;
}
int convertd (int n)
{
    int i=1,sum=0;
    while (n>0)
    {
        int d=n%2;
        sum+=d*i;
        i*=10;
        n/=2;
    }
    return sum;
}
int main()
{
    int num1,num2;
    printf("Enter 1st binary number: ");
    scanf("%d",&num1);
    printf("Enter 2nd binary number: ");
    scanf("%d",&num2);
    int result=convertb(num1)+convertb(num2);
    printf("Result: %d\n",convertd(result));
    return 0;
}
