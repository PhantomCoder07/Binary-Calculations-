#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <math.h>
int convertb (int n)
{
    int i=0,sum=0;
    while (n!=0)
    {
        int d=n%10;
        if (d==1)
        {
            sum+=pow(2,i);
        }
        n/=10;
        i++;
    }
    return sum;
}
int convertd (int n)
{
    int i=1,sum=0;
    while (n>0)
    {
        int d=n%2;
        sum+=d*i;
        i*=10;
        n/=2;
    }
    return sum;
}
int main()
{
    char str1[100],str2[100];
    printf("Enter 1st binary number: ");
    gets(str1);
    printf("Enter 2nd binary number: ");
    gets(str2);
    int num1=atoi(str1);
    int num2=atoi(str2);
    int result=convertb(num1)+convertb(num2);
    printf("Result: %d\n",convertd(result));
    return 0;
}
